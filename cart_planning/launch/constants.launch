<?xml version="1.0"?>

<launch>
    <arg name="graph_file" default="$(find cart_planning)/data/fixed.gml"/>
    <arg name="graph_file_broken_cafe" default="$(find cart_planning)/data/research_broken_1.gml"/>
    <arg name="graph_file_broken_ehall" default="$(find cart_planning)/data/temp_broken/ResearchBroken_EHall.gml"/>
    <arg name="graph_file_broken_loop" default="$(find cart_planning)/data/temp_broken/ResearchBroken_Loop.gml"/>
    <arg name="graph_file_broken_loop2" default="$(find cart_planning)/data/temp_broken/ResearchBroken_LoopV2.gml"/>
    <arg name="graph_file_broken_rock" default="$(find cart_planning)/data/temp_broken/ResearchBroken_Rock.gml"/>
    <arg name="graph_file_broken_rock2" default="$(find cart_planning)/data/temp_broken/ResearchBroken_RockV2.gml"/>
    <!-- Global Planner graph file parameter -->
    <rosparam param="graph_file1" subst_value="True">$(arg graph_file)</rosparam>
    <rosparam param="graph_file2" subst_value="True">$(arg graph_file_broken_cafe)</rosparam>
    <rosparam param="graph_file3" subst_value="True">$(arg graph_file_broken_ehall)</rosparam>
    <rosparam param="graph_file4" subst_value="True">$(arg graph_file_broken_loop)</rosparam>
    <rosparam param="graph_file5" subst_value="True">$(arg graph_file_broken_rock)</rosparam>
    <rosparam param="graph_file6" subst_value="True">$(arg graph_file_broken_loop2)</rosparam>
    <rosparam param="graph_file7" subst_value="True">$(arg graph_file_broken_rock2)</rosparam>


    <rosparam>
        <!-- GPS Util Params -->
        anchor_gps: [38.433939, -78.862157]
        anchor_theta : 100
        <!-- GPS Calibration Params -->
        anchor_local: [0, 0]
        test_loc_gps: [38.433170, -78.860981]
        test_loc_local: [67.6, 115]

        <!-- Safety Parameters -->
        <!-- These are reactive in the sense that if the max_speed is reached the cart will emergency stop -->
        max_speed: 5
        max_ndt_health: 4
        
        <!-- Collision Detection Parameters -->
        <!-- Minimum distance an obstacle can be from the cart before emergency stopping-->
        min_obstacle_dist: 0.5
        <!-- Time(seconds) away an obstacle can be before emergency stopping-->
        min_obstacle_time: 0.5

        <!-- Safe distance before a general eased stop -->
        safe_obstacle_dist: 6
        safe_obstacle_time: 2

        <!-- Vehicle Kinematics (Meters) -->
        vehicle_width: 1.1938
        vehicle_length: 2.4003 
        wheel_base: 2.4003
        front_axle_track: 0.9017
        rear_axle_track: 0.9652
        tire_width: 0.2159
    </rosparam>
</launch>
